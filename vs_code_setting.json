{
"breadcrumbs.enabled": true,
"css.validate": false,
"diffEditor.ignoreTrimWhitespace": true,
"editor.cursorBlinking": "solid",
"editor.cursorStyle": "block",
"editor.fastScrollSensitivity": 3,
"editor.fontLigatures": true,
"editor.fontSize": 14,
"editor.formatOnPaste": true,
"editor.formatOnSave": true,
"editor.formatOnSaveTimeout": 3000,
"editor.insertSpaces": true,
"editor.minimap.enabled": false,
"editor.quickSuggestions": {
"comments": true,
"other": true,
"strings": true
  },
"editor.quickSuggestionsDelay": 0,
"editor.renderControlCharacters": false,
"editor.renderWhitespace": "all",
"editor.smoothScrolling": true,
"editor.snippetSuggestions": "top",
"editor.suggestSelection": "first",
"editor.tabCompletion": "on",
"editor.tabSize": 4,
"files.insertFinalNewline": true,
"files.trimTrailingWhitespace": true,
"go.autocompleteUnimportedPackages": true,
"go.useLanguageServer": true,
"todo-tree.customHighlight": {
"FIXME": {
"background": "#EE6492",
"icon": "alert",
"iconColour": "#EE6492"
    },
"TODO": {
"icon": "check"
    }
  },
"todo-tree.defaultHighlight": {
"background": "#EFAC3A",
"foreground": "#FEF7FA",
"icon": "check",
"iconColour": "#EFAC3A",
"type": "tag"
  },
"todo-tree.tags": ["TODO:", "FIXME:"],
"vim.easymotion": true,
"vim.easymotionMarkerFontFamily": "Hack",
"vim.easymotionMarkerFontSize": "16",
"vim.easymotionMarkerHeight": 25,
"vim.easymotionMarkerWidthPerChar": 12,
"vim.easymotionMarkerYOffset": 8,
"vim.foldfix": true,
"vim.sneak": true,
"vim.sneakUseIgnorecaseAndSmartcase": true,
"vim.statusBarColorControl": false,
"vim.useSystemClipboard": true,
"editor.renderLineHighlight": "line",
"workbench.colorCustomizations": {
  "editor.lineHighlightBackground": "#0B0B3B"
  }
}
